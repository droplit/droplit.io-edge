{
    "transport": {
        "host": "wss://edge-ws.droplit.io/edge"
    },
    "ecosystemId": "INSERT_YOUR_ECOSYSTEMID_HERE",
    "plugins": {
        "droplit-plugin-philips-hue": {
            "enabled": true
        },
        "droplit-plugin-lifx": {},
        "droplit-plugin-sonos": {},
        "droplit-plugin-wemo": {},
        "droplit-plugin-local-ts-example": {
            "enabled": true,
            "localServices": [
                "Host"
            ]
        }
    },
       "promotedMembers": {
            "motd" : "Host.motd",
            "announce": "Host.announce"
    }
}